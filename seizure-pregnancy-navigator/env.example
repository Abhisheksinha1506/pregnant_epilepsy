# API Integration Environment Variables
# Copy this file to .env.local and fill in your actual API keys

# OpenFDA API Configuration
OPENFDA_API_KEY=your_openfda_api_key_here
OPENFDA_BASE_URL=https://api.fda.gov

# NIH DailyMed API Configuration
NIH_API_KEY=your_nih_api_key_here
NIH_BASE_URL=https://dailymed.nlm.nih.gov

# CDC API Configuration
CDC_API_KEY=your_cdc_api_key_here
CDC_BASE_URL=https://data.cdc.gov

# WHO API Configuration (if available)
WHO_API_KEY=your_who_api_key_here
WHO_BASE_URL=https://apps.who.int

# Epilepsy Foundation API Configuration (if available)
EPILEPSY_FOUNDATION_API_KEY=your_epilepsy_foundation_api_key_here
EPILEPSY_FOUNDATION_BASE_URL=https://www.epilepsy.com

# Rate Limiting Configuration
API_RATE_LIMIT=100
API_RATE_WINDOW=60000

# Cache Configuration
CACHE_TTL=300000
CACHE_MAX_SIZE=1000

# Development Settings
NODE_ENV=development
DEBUG_API_CALLS=true
