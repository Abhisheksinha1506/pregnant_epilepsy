{
  "version": 2,
  "buildCommand": "cd seizure-pregnancy-navigator && npm run build",
  "installCommand": "cd seizure-pregnancy-navigator && npm install",
  "outputDirectory": "seizure-pregnancy-navigator/.next",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/seizure-pregnancy-navigator/$1"
    }
  ],
  "functions": {
    "seizure-pregnancy-navigator/app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}

